    "修为历练类别": {
      "cases": [
        {
          "name": "风火历练",
          "pipeline_override": {
            "选择你的修为历练": {
              "next": ["进入你的风火历练"]
            }
          }
        },
        {
          "name": "地水历练",
          "pipeline_override": {
