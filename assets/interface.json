            "找到铜钱历练具体层数": {
              "expected": "王庭贸易"
            }
          }
        }
      ]
    },
    "铜钱历练次数": {
      "cases": [
        {
          "name": "1",
          "pipeline_override": {
            "增加铜钱历练扫荡次数至": {
              "expected": "1"
            }
          }
        },
        {
          "name": "2",
          "pipeline_override": {
            "增加铜钱历练扫荡次数至": {
              "expected": "2"
            }
          }
        },
        {
          "name": "3",
          "pipeline_override": {
            "增加铜钱历练扫荡次数至": {
              "expected": "3"
