{
  "name": "MaaYuan",
  "custom_title": "MAA鸢",
  "default_controller": "adb",

  "controller": [
    {
      "name": "安卓端",
      "type": "Adb"
    },
    {
      "name": "桌面端",
      "type": "Win32",
      "win32": {
        "class_regex": ".*",
        "window_regex": "Visual Studio"
      }
    }
  ],
  "resource": [
    {
      "name": "简中服",
      "path": ["{PROJECT_DIR}/resource"]
    }
  ],
  "task": [
    {
      "name": "启动游戏",
      "entry": "启动游戏版本",
      "option": ["请选择游戏版本"],
      "check": true,
      "doc": "如鸢官服即官网下载，如鸢Tap服即TapTap下载，如启动不了请提交来源QAQ"
    },
    {
      "name": "开始游戏",
      "entry": "启动游戏",
      "check": true,
      "doc": "运行游戏后从启动页面到主页面"
    },
    {
      "name": "突发情况（漆园蝶）",
      "entry": "start_突发情况启动",
      "check": true,
      "option": ["循环开关（自动吃蛇肉）"],
      "doc": "进行一键日常时不要开启自动吃蛇肉！"
    },
    {
      "name": "小道消息",
      "entry": "start_小道消息",
      "check": true,
      "option": ["小道消息_循环开关"]
    },
    {
      "name": "他的传闻",
      "entry": "start_他的传闻启动",
      "check": true,
      "option": ["循环开关（自动吃茱萸）"],
      "doc": "进行一键日常时不要开启自动吃茱萸！"
    },
    {
      "name": "待办公务",
      "entry": "start_待办公务启动",
      "check": true,
      "option": ["公务类型", "决策事件选择", "待办公务_循环开关"],
      "doc": ""
    },
    {
      "name": "收取据点资源",
      "entry": "start_收取据点",
      "check": true
    },
    {
      "name": "相见互动",
      "entry": "相见互动启动",
      "check": true
    },
    {
      "name": "6-24",
      "entry": "6-24开始位置",
      "option": ["6-24开始位置"]
    }
  ],
  "option": {
    "6-24开始位置": {
      "cases": [
        {
          "name": "6-24关卡外",
          "pipeline_override": {
            "6-24开始位置": {
              "next": ["进入界面-主线故事-第六章-24"]
            }
          }
        },
        {
          "name": "6-24关卡内",
          "pipeline_override": {
            "6-24开始位置": {
              "next": ["战斗6-24"]
            }
          }
        }
      ]
    },
    "循环开关（自动吃蛇肉）": {
      "cases": [
        {
          "name": "关",
          "pipeline_override": {
            "气力值回复": {
              "next": ["stop"]
            },
            "end_当前没有突发情报": {
              "next": ["返回上级界面"]
            }
          }
        },
        {
          "name": "开",
          "pipeline_override": {
            "end_当前没有突发情报": {
              "next": ["等待新的突发情报", "stop"]
            }
          }
        }
      ]
    },
    "小道消息_循环开关": {
      "cases": [
        {
          "name": "关",
          "pipeline_override": {
            "气力值回复": {
              "next": ["stop"]
            },
            "end_小道消息循环开关": {
              "next": ["返回上级界面"]
            }
          }
        },
        {
          "name": "开",
          "pipeline_override": {
            "end_小道消息循环开关": {
              "next": ["loop_小道消息", "小道消息收集中"]
            }
          }
        }
      ]
    },
    "循环开关（自动吃茱萸）": {
      "cases": [
        {
          "name": "关",
          "pipeline_override": {
            "new气力值回复": {
              "next": ["stop"]
            },
            "end_是否继续他的传闻": {
              "next": ["返回上级界面"]
            }
          }
        },
        {
          "name": "开",
          "pipeline_override": {
            "end_是否继续他的传闻": {
              "next": ["他的传闻点击调查"]
            }
          }
        }
      ]
    },
    "公务类型": {
      "cases": [
        {
          "name": "作战+建材",
          "pipeline_override": {
            "待办公务可做任务": {
              "next": ["识别请求出战", "识别包含建材的物资支援"]
            }
          }
        },
        {
          "name": "所有公务",
          "pipeline_override": {
            "待办公务可做任务": {
              "next": ["识别请求出战", "识别物资支援", "识别决策事件"]
            }
          }
        },
        {
          "name": "作战+决策",
          "pipeline_override": {
            "待办公务可做任务": {
              "next": ["识别请求出战", "识别决策事件"]
            }
          }
        }
      ]
    },
    "决策事件选择": {
      "cases": [
        {
          "name": "经验",
          "pipeline_override": {
            "决策事件选择": {
              "roi": [487, 511, 142, 49]
            }
          }
        },
        {
          "name": "五铢钱",
          "pipeline_override": {
            "决策事件选择": {
              "roi": [487, 745, 139, 42]
            }
          }
        }
      ]
    },
    "待办公务_循环开关": {
      "cases": [
        {
          "name": "关",
          "pipeline_override": {
            "气力值回复": {
              "next": ["stop"]
            },
            "end_当前没有可办公务": {
              "next": ["返回上级界面"]
            }
          }
        },
        {
          "name": "开",
          "pipeline_override": {
            "end_当前没有可办公务": {
              "next": ["识别下一份鸢报", "stop"]
            }
          }
        }
      ]
    },
    "请选择游戏版本": {
      "cases": [
        {
          "name": "如鸢官服",
          "pipeline_override": {
            "如鸢官服": {
              "enabled": true
            }
          }
        },
        {
          "name": "如鸢Tap服",
          "pipeline_override": {
            "如鸢Tap服": {
              "enabled": true
            }
          }
        },
        {
          "name": "代号鸢港服",
          "pipeline_override": {
            "代号鸢港服": {
              "enabled": true
            }
          }
        }
      ]
    }
  }
}
