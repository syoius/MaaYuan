{
  "name": "MaaYuan",
  "custom_title": "MAA鸢",
  "default_controller": "adb",

  "controller": [
    {
      "name": "安卓端",
      "type": "Adb"
    },
    {
      "name": "桌面端",
      "type": "Win32",
      "win32": {
        "class_regex": ".*",
        "window_regex": "Visual Studio"
      }
    }
  ],
  "resource": [
    {
      "name": "简中服",
      "path": ["{PROJECT_DIR}/resource"]
    }
  ],
  "task": [
    {
      "name": "启动游戏",
      "entry": "启动游戏版本",
      "option": ["请选择游戏版本"],
      "check": true,
      "doc": "如鸢官服即官网下载，如鸢Tap服即TapTap下载，如启动不了请提交来源QAQ"
    },
    {
      "name": "突发情况（漆园蝶）",
      "entry": "start_突发情况启动",
      "check": true,
      "option": ["循环开关（自动吃蛇肉）"],
      "doc": "进行一键日常时不要开启自动吃蛇肉！"
    },
    {
      "name": "小道消息",
      "entry": "start_小道消息",
      "check": true,
      "option": ["小道消息_循环开关"]
    },
    {
      "name": "他的传闻",
      "entry": "start_他的传闻启动",
      "check": true,
      "option": ["循环开关（自动吃茱萸）"],
      "doc": "进行一键日常时不要开启自动吃茱萸！"
    },
    {
      "name": "待办公务",
      "entry": "start_待办公务启动",
      "check": true,
      "option": ["公务类型", "决策事件选择", "待办公务_循环开关"],
      "doc": ""
    },
    {
      "name": "【需月卡】领体力福利",
      "entry": "领体力启动",
      "check": true
    },
    {
      "name": "每日进膳",
      "entry": "每日进膳启动",
      "check": true
    },
    {
      "name": "派遣-洛阳",
      "entry": "派遣洛阳启动"
    },
    {
      "name": "派遣-寿春",
      "entry": "派遣寿春启动"
    },
    {
      "name": "派遣-广陵",
      "entry": "派遣广陵启动"
    },
    {
      "name": "收据点+三星情报（非月卡党点→⚙改设置）",
      "entry": "据点情报启动",
      "check": true,
      "option": ["据点情报处理方式"],
      "doc": "不管有没有月卡，还没做过的情报都会通过自动走格子完成。"
    },
    {
      "name": "铜钱历练",
      "entry": "刷铜钱历练启动",
      "option": ["铜钱历练级别", "铜钱历练次数"]
    },
    {
      "name": "经验历练",
      "entry": "刷经验历练启动",
      "option": ["经验历练级别", "经验历练次数"]
    },
    {
      "name": "修为历练",
      "entry": "刷修为历练启动",
      "option": ["修为历练类别", "修为历练级别", "修为历练次数"]
    },
    {
      "name": "相见互动",
      "entry": "相见互动启动",
      "check": true
    },
    {
      "name": "家具互动",
      "entry": "家具互动启动",
      "check": true
    },
    {
      "name": "每日分享",
      "entry": "每日分享启动",
      "check": true
    },
    {
      "name": "6-24",
      "entry": "start_6-24",
      "option": ["start_6-24"]
    },
    {
      "name": "【如鸢】三千世界刷成就",
      "entry": "三千世界版刷成就",
      "doc": "请自己准备好自动战斗的队伍，在关卡界面（选普通/困难/噩梦难度那里）启动。推荐关卡-黄金马车"
    },
    {
      "name": "【代号鸢】隆地冬自动历险",
      "entry": "隆地东自动走历险",
      "doc": "因为每次历险都要求重新编队，建议先自己走到第一次战斗，在编队界面编好队后，再启动这个任务。之后同一次历险期间（比如自动战斗失败了、手动换队才打过之后想继续挂机），可以在走路界面启动这个任务，会自动走完后面的~"
    }
  ],
  "option": {
    "start_6-24": {
      "cases": [
        {
          "name": "6-24关卡外",
          "pipeline_override": {
            "start_6-24": {
              "next": ["进入6-24"]
            }
          }
        },
        {
          "name": "6-24关卡内",
          "pipeline_override": {
            "start_6-24": {
              "next": ["战斗6-24"]
            }
          }
        }
      ]
    },
    "循环开关（自动吃蛇肉）": {
      "cases": [
        {
          "name": "关",
          "pipeline_override": {
            "气力值回复": {
              "next": ["stop"]
            },
            "end_当前没有突发情报": {
              "next": ["返回上级界面"]
            }
          }
        },
        {
          "name": "开",
          "pipeline_override": {
            "end_当前没有突发情报": {
              "next": ["等待新的突发情报"]
            }
          }
        }
      ]
    },
    "小道消息_循环开关": {
      "cases": [
        {
          "name": "关",
          "pipeline_override": {
            "气力值回复": {
              "next": ["stop"]
            },
            "end_小道消息循环开关": {
              "next": ["返回上级界面"]
            }
          }
        },
        {
          "name": "开",
          "pipeline_override": {
            "end_小道消息循环开关": {
              "next": ["loop_小道消息"]
            }
          }
        }
      ]
    },
    "循环开关（自动吃茱萸）": {
      "cases": [
        {
          "name": "关",
          "pipeline_override": {
            "new气力值回复": {
              "next": ["stop"]
            },
            "end_是否继续他的传闻": {
              "next": ["返回上级界面"]
            }
          }
        },
        {
          "name": "开",
          "pipeline_override": {
            "end_是否继续他的传闻": {
              "next": ["他的传闻点击调查"]
            }
          }
        }
      ]
    },
    "公务类型": {
      "cases": [
        {
          "name": "作战+建材",
          "pipeline_override": {
            "待办公务可做任务": {
              "next": ["识别请求出战", "识别包含建材的物资支援"]
            }
          }
        },
        {
          "name": "所有公务",
          "pipeline_override": {
            "待办公务可做任务": {
              "next": ["识别请求出战", "识别物资支援", "识别决策事件"]
            }
          }
        },
        {
          "name": "作战+决策",
          "pipeline_override": {
            "待办公务可做任务": {
              "next": ["识别请求出战", "识别决策事件"]
            }
          }
        }
      ]
    },
    "决策事件选择": {
      "cases": [
        {
          "name": "经验",
          "pipeline_override": {
            "决策事件选择": {
              "roi": [487, 511, 142, 49]
            }
          }
        },
        {
          "name": "五铢钱",
          "pipeline_override": {
            "决策事件选择": {
              "roi": [487, 745, 139, 42]
            }
          }
        }
      ]
    },
    "待办公务_循环开关": {
      "cases": [
        {
          "name": "关",
          "pipeline_override": {
            "气力值回复": {
              "next": ["stop"]
            },
            "end_当前没有可办公务": {
              "next": ["返回上级界面"]
            }
          }
        },
        {
          "name": "开",
          "pipeline_override": {
            "end_当前没有可办公务": {
              "next": ["识别下一份鸢报", "stop"]
            }
          }
        }
      ]
    },
    "请选择游戏版本": {
      "cases": [
        {
          "name": "如鸢官服",
          "pipeline_override": {
            "如鸢官服": {
              "enabled": true
            }
          }
        },
        {
          "name": "如鸢Tap服",
          "pipeline_override": {
            "如鸢Tap服": {
              "enabled": true
            }
          }
        },
        {
          "name": "代号鸢港服",
          "pipeline_override": {
            "代号鸢港服": {
              "enabled": true
            }
          }
        }
      ]
    },
    "铜钱历练级别": {
      "cases": [
        {
          "name": "巨额贸易（国服最高）",
          "pipeline_override": {
            "找到铜钱历练具体层数": {
              "expected": "巨额贸易"
            }
          }
        },
        {
          "name": "巴蜀贸易",
