          }
        },
        {
          "name": "4",
          "pipeline_override": {
            "增加铜钱历练扫荡次数至": {
              "expected": "4"
            }
          }
        },
        {
          "name": "5",
          "pipeline_override": {
            "增加铜钱历练扫荡次数至": {
              "expected": "5"
            }
          }
        },
        {
          "name": "6",
          "pipeline_override": {
            "增加铜钱历练扫荡次数至": {
              "expected": "6"
            }
          }
        }
      ]
    },
    "经验历练级别": {
      "cases": [
        {
          "name": "终极历练（国服最高）",
          "pipeline_override": {
            "找到经验历练具体层数": {
              "expected": "终极历练"
            }
          }
        },
        {
          "name": "实战历练",
          "pipeline_override": {
            "找到经验历练具体层数": {
              "expected": "实战历练"
            }
          }
        },
        {
          "name": "鏖战历练",
          "pipeline_override": {
            "找到经验历练具体层数": {
              "recognition": "TemplateMatch",
              "template": "aozhan.png",
              "threshold": 0.95
            }
          }
        },
        {
          "name": "绝境历练（港服最高）",
          "pipeline_override": {
            "找到经验历练具体层数": {
              "expected": "绝境历练"
            }
          }
        }
      ]
    },
    "经验历练次数": {
      "cases": [
        {
          "name": "1",
          "pipeline_override": {
            "增加经验历练扫荡次数至": {
