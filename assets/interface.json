    "据点情报处理方式": {
      "cases": [
        {
          "name": "扫荡",
          "pipeline_override": {
            "找到当前列表的三星情报": {
              "next": ["找到三星情报行动入口"]
            },
            "找到郿坞列表的三星情报": {
              "next": ["找到郿坞三星情报行动入口"]
            }
          }
        },
        {
          "name": "自动走格子",
          "pipeline_override": {
            "找到当前列表的三星情报": {
              "next": ["找到三星情报行动入口-无月卡"]
            },
            "找到郿坞列表的三星情报": {
              "next": ["找到郿坞三星情报行动入口-无月卡"]
            }
          }
        }
      ]
    }
  }
