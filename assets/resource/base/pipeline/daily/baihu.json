{
  "白鹄扫荡启动": {
    "recognition": {
      "type": "DirectHit",
      "param": {}
    },
    "action": {
      "type": "DoNothing",
      "param": {}
    },
    "post_wait_freezes": 2000,
    "next": [
      "寻找白鹄界面",
      "[JumpBack]兰台每日弹窗",
      "[JumpBack]进入界面-兰台"
    ]
  },
  "寻找白鹄界面": {
    "recognition": {
      "type": "TemplateMatch",
      "param": {
        "template": "baihu/icon.png",
        "roi": [0, 836, 126, 432]
      }
    },
    "action": {
      "type": "DoNothing",
      "param": {}
    },
    "post_delay": 500,
    "next": ["点击进入白鹄界面", "[JumpBack]兰台每日弹窗"]
  },
  "点击进入白鹄界面": {
    "recognition": {
      "type": "TemplateMatch",
      "param": {
        "template": "baihu/icon.png",
        "roi": [0, 836, 126, 432]
      }
    },
    "action": {
      "type": "Click",
      "param": {}
    },
    "pre_delay": 200,
    "timeout": 7000,
    "next": ["无行动次数check", "寻找白鹄扫荡界面", "[JumpBack]兰台每日弹窗"],
    "on_error": ["白鹄行动未开放"]
  },
  "白鹄行动未开放": {
    "recognition": {
      "type": "DirectHit",
      "param": {}
    },
    "action": {
      "type": "DoNothing",
      "param": {}
    },
    "focus": "白鹄行动未开放，行动中止",
    "next": ["stop"]
  },
  "无行动次数check": {
    "recognition": {
      "type": "OCR",
      "param": {
        "expected": "0",
        "replace": ["O", "0"],
        "roi": [568, 272, 59, 62]
      }
    },
    "action": {
      "type": "DoNothing",
      "param": {}
    },
    "focus": "检测到白鹄无行动次数，行动中止",
    "next": ["白鹄扫荡进入奖励领取界面", "stop"]
  },
  "寻找白鹄扫荡界面": {
    "recognition": {
      "type": "OCR",
      "param": {
        "expected": "扫荡",
        "roi": [123, 1005, 159, 62]
      }
    },
    "action": {
      "type": "DoNothing",
      "param": {}
    },
    "pre_delay": 500,
    "next": ["无行动次数check", "点击白鹄扫荡界面"]
  },
  "点击白鹄扫荡界面": {
    "recognition": {
      "type": "OCR",
      "param": {
        "expected": "扫荡",
        "roi": [123, 1005, 159, 62]
      }
    },
    "action": {
      "type": "Click",
      "param": {}
    },
    "pre_delay": 500,
    "next": ["设置白鹄扫荡次数"]
  },
  "设置白鹄扫荡次数": {
    "recognition": {
      "type": "OCR",
      "param": {
        "expected": "最多",
        "roi": [505, 606, 75, 62]
      }
    },
    "action": {
      "type": "Click",
      "param": {}
    },
    "pre_delay": 500,
    "next": ["点击进行白鹄扫荡"]
  },
  "点击进行白鹄扫荡": {
    "recognition": {
      "type": "OCR",
      "param": {
        "expected": "开始扫荡",
        "roi": [271, 777, 174, 55]
      }
    },
    "action": {
      "type": "Click",
      "param": {}
    },
    "pre_delay": 500,
    "next": ["白鹄扫荡结果确认", "白鹄扫荡异常进入"]
  },
  "白鹄扫荡结果确认": {
    "recognition": {
      "type": "OCR",
      "param": {
        "expected": "扫荡结果"
      }
    },
    "action": {
      "type": "Click",
      "param": {
        "target": [670, 220, 18, 19]
      }
    },
    "pre_delay": 500,
    "focus": "白鹄扫荡完成",
    "next": ["白鹄扫荡进入奖励领取界面"]
  },
  "白鹄扫荡异常进入": {
    "recognition": {
      "type": "OCR",
      "param": {
        "expected": "取消",
        "roi": [150, 639, 133, 97]
      }
    },
    "action": {
      "type": "Click",
      "param": {}
    },
    "pre_delay": 500,
    "focus": "白鹄扫荡异常进入!已取消！",
    "next": ["stop"]
  },
  "白鹄扫荡进入奖励领取界面": {
    "recognition": {
      "type": "TemplateMatch",
      "param": {
        "template": "baihu/reward.png",
        "roi": [338, 271, 58, 62]
      }
    },
    "action": {
      "type": "Click",
      "param": {}
    },
    "pre_delay": 200,
    "timeout": 2000,
    "next": ["白鹄扫荡奖励领取"],
    "on_error": ["返回上级界面2"]
  },
  "白鹄扫荡奖励领取": {
    "recognition": {
      "type": "TemplateMatch",
      "param": {
        "template": "baihu/pick.png",
        "roi": [471, 390, 173, 547]
      }
    },
    "action": {
      "type": "Click",
      "param": {}
    },
    "pre_delay": 500,
    "next": ["白鹄扫荡退出奖励领取"]
  },
  "白鹄扫荡退出奖励领取": {
    "recognition": {
      "type": "TemplateMatch",
      "param": {
        "template": "common_reward.png",
        "roi": [5, 3, 713, 526],
        "threshold": 0.6,
        "green_mask": true
      }
    },
    "action": {
      "type": "Click",
      "param": {
        "target": [633, 22, 19, 11]
      }
    },
    "pre_delay": 500,
    "post_delay": 3000,
    "next": ["白鹄扫荡奖励领取", "白鹄扫荡完成"]
  },
  "白鹄扫荡完成": {
    "recognition": {
      "type": "DirectHit",
      "param": {}
    },
    "action": {
      "type": "Click",
      "param": {
        "target": [633, 22, 19, 11]
      }
    },
    "pre_delay": 500,
    "next": ["stop"]
  }
}
